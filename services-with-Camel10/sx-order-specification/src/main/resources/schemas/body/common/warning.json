            "type": "string"
        },
        "messageCode": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "category": {
            "type": "string"
        },
        "warningNature": {
            "enum": [
                "None",
